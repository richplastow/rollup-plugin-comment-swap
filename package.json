{
  "name": "rollup-plugin-comment-swap",
  "version": "0.0.1",
  "description": "A powerful and flexible Rollup plugin for injecting values and code into bundled files",
  "license": "MIT",
  "author": "Rich Plastow",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "type": "commonjs",
  "exports": {
    "types": "./types/index.d.ts",
    "import": "./dist/es/index.js",
    "default": "./dist/cjs/index.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
